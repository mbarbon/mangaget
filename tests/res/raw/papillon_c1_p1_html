	$('#bookmarkbutton').bind('click', function() {
		$.get("/bookmark.php", { mangaid: "5338", chapterid: "98453", page: "1", action: "add" },
		   function(data){
			   if(data == "done")
			   {
				$('#bookmarkbutton').css("background-image", "url(/img/fam/tick.png)");
				$('#bookmarkbutton').html("Bookmarked!");
			   }
			   else
			   {
					alert("You need to be logged in to bookmark.");   
			   }
		   });
	});
	$('#deadbutton').bind('click', function() {
		$.get("/dead.php", { mangaid: "5338", chapterid: "98453", page: "2598070" },
		   function(data){
			   if(data == "done")
			   {
				$('#deadbutton').css("background-image", "url(/img/fam/tick.png)");
				$('#deadbutton').html("Thanks for reporting!");
			   }
			   else
			   {
					alert("You need to be logged in to report a dead page.");   
			   }
		   });
	});
});
