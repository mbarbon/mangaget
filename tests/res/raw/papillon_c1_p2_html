<span style="background-image:url('http://s2-c.animea-server.net/5338%2F1_JHMCN%2F002_inside.jpg'); width:0px; height:0px;"><span><script type="text/javascript">
function init_events(back, next, enable_back, enable_next) {
    document.onkeydown = function(e) {
        e = e || window.event;
        var keycode = e.keyCode || e.which;

        if (enable_next && 39 == keycode) {
            if (next)
                window.location = next;
            return false;
        } else if (enable_back && 37 == keycode) {
            if (back)
                window.location = back;
            else
                alert("This is the first page.");
            return false;
        }
    };

    document.onclick = function(e) {
        e = e || window.event;
        var target = e.target || e.srcElement;

        switch (target.id) {
            case "btn-back-1":
            case "btn-back-2":
                if (enable_back && back) {
                    window.location = back;
                    return false;
                }
                break;
            case "btn-next-1":
            case "btn-next-2":
                if (enable_next && next) {
                    window.location = next;
                    return false;
                }
                break;
        }
    };
}

var next = 'http://manga.animea.net/papillon-hana-to-chou-chapter-1-page-3.html';


var back = 'http://manga.animea.net/papillon-hana-to-chou-chapter-1-page-1.html';
init_events(back, next, true, true);

</script>
