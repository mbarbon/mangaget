		});
	});
	$("#bookmark2").bind("click", function () {
		$.ajax({
			type: "POST",
			url: "/ajax/bookmark.php",
			data: "action=add&series_id=5793&chapter_id=82094&page=1",
			success: function (msg) {
				if (msg == 1) {
					alert("You need to login first!");
					location.href = '/login.htm';
				} else {
					alert(msg);
				}
			}
		});
	});
	$("#report_image").bind("click", function () {
		var con1 = window.confirm("Want to report image load problem for " + series_name + " " + current_chapter + "/ " + current_page + "?");
		if (con1) {
			$.ajax({
				type: "POST",
				url: "/ajax/reports.php",
				data: "url=http://www.mangahere.com/manga/kyoukai_no_rinne/v01/c002/1.html&status=1",
				success: function (msg) {
					alert(msg);
				}
			});
		}
	});
	$("#report_content").bind("click", function () {
		var con2 = window.confirm("Want to report inappropriate or mature content on " + series_name + " " + current_chapter + "/ " + current_page + "?");
		if (con2) {
			$.ajax({
				type: "POST",
				url: "/ajax/reports.php",
				data: "url=http://www.mangahere.com/manga/kyoukai_no_rinne/v01/c002/1.html&status=0",
				success: function (msg) {
					alert(msg);
				}
			});
		}
	});
	$("#subscribe").bind("click", function () {
    $('#email_post').show();
    $('#email').val('');
    $('#email').focus();
	});
  $("#subscribe_b").bind("click", function(){
		$.ajax({
			type: "POST",
			url: "/ajax/subscribe.php",
			data: "action=subscribe&series_id=5793&email="+$("#email").val(),
			success: function (msg) {
				if (msg == 1) {
          $('#email').focus();
          alert("Please input an email address!");
				} else if (msg == 2) {
          $('#email').focus();
					alert("Invalid email address!");
				} else {
          $('#email_post').hide();
					alert(msg);
				}
			}
		});
  });
	$("#close_email_post").bind("click", function () {
    $('#email_post').hide();
	});
	(function(d, s, id) {
		var js, fjs = d.getElementsByTagName(s)[0];
		if (d.getElementById(id)) return;
		js = d.createElement(s); js.id = id;
		js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
		fjs.parentNode.insertBefore(js, fjs);
	}(document, 'script', 'facebook-jssdk'));
});
</script>
