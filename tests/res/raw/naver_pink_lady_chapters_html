<script type="text/javascript">
	var isLogin = 'false';
	var isAdultWebtoon = 'N';
	var isOver19 = 'false';
	var isParty = 'true';
	var levelName = "webtoon";
	
	var artistData = [
       
          {"artistId" : 79,
           "nickname": '연우'}
            
       
    ];

    var actionRunner;


    WebtoonArtistActionRunner = $Class ({
        $init : function(titleId) {
            this._titleId = titleId;
	    },

	    artistForFavorite : function(element) {
	    	$Element("author_layer_inner").html("");
	    	$Element("author_layer").css("border", "");
            $Element("author_layer_inner").css("border", "1px solid #b6b6b6");
	    	for (var i = 0; i < artistData.length; i++) {
		    	var liElement = new $Element("<li>");
		    	var aElement = new $Element("<a>");
		    	aElement.attr("href", "javascript:actionRunner.addFavoriteArtist('" + artistData[i].artistId + "')");
		    	aElement.html("관심작가 등록&lt;" + artistData[i].nickname + "&gt;")

		    	liElement.append(aElement);
		    	
		    	$Element("author_layer_inner").append(liElement);

                if (i==9) {
                    $Element("author_layer").css("border", "1px solid #b6b6b6");
                    $Element("author_layer_inner").css("border", "0");
                }
	    	}
		    artistLayer.show(element);
	    },
	  	//관심 웹툰 추가
        addFavoriteWebtoon : function (titleId) {
	    	var width = (screen.width / 2) - 800;
			var height = (screen.height / 2) - 510;
		    if (!false) {
			    if(confirm('로그인 해야 이용할 수 있습니다. 로그인 하시겠습니까?')) {
			    	window.open("https://nid.naver.com/nidlogin.login?svctype=64&url=http://comic.naver.com/mypage/favoriteArtist.nhn?m=insert%26titleId="+titleId,"addFavorite" ,"toolbar=no,scrollbars=no,width=370,height=170,top="+height+",left="+width);
				}
		    } else {
				window.open("/mypage/favoriteArtist.nhn?m=insert&titleId="+titleId,"addFavorite" ,"toolbar=no,scrollbars=no,width=370,height=170,top="+height+",left="+width);
		    }
	    },
	    
	    artistForArtistTitle : function(element) {
	    	  $Element("author_layer_inner").html("");
	    	  $Element("author_layer").css("border", "");
	          $Element("author_layer_inner").css("border", "1px solid #b6b6b6");
	          for (var i = 0; i < artistData.length; i++) {
	              var liElement = new $Element("<li>");
	              var aElement = new $Element("<a>");
	              aElement.attr("href", "/artistTitle.nhn?artistId=" + artistData[i].artistId);
	              aElement.html("&lt;" + artistData[i].nickname + "&gt;의 다른작품");

	              liElement.append(aElement);
	                
	              $Element("author_layer_inner").append(liElement);

                  if (i==9) {
                      $Element("author_layer").css("border", "1px solid #b6b6b6");
                      $Element("author_layer_inner").css("border", "0");
                  }
              }
	          artistLayer.show(element);
	    },
	     
	    addFavoriteArtist : function(artistId) {
		    if (!isLogin) {
			    if( confirm('로그인 해야 이용할 수 있습니다. 로그인 하시겠습니까?') ) {
			        location.href = "http://nid.naver.com/nidlogin.login?mode=form&url="+encodeURIComponent(location.href);
                }
		    } else {
		        location.href = '/mypage/favoriteArtist.nhn?m=insert&artistId=' + artistId +
		                        '&titleId=' + this._titleId + '&nextUrl=' + encodeURIComponent(location.href)  ;
		    }
                
	    }
	});
			
	
	function loadThisWindow () {
	    actionRunner = new WebtoonArtistActionRunner("22896");
	}

	var popupObject = new nhn_naver_commcast_feed_popup();  // 팝업처리 객체 생성

	function applyFeed(){
		if(isAdultWebtoon == 'Y'){
			if (isLogin == 'false') {
				location.href = "http://nid.naver.com/nidlogin.login?viewtype=3&svctype=128&url="+encodeURIComponent(location.href);
			}else if ( (isLogin == 'true') && (isOver19 == 'false') ){
				location.href = "/"+levelName+"/detail.nhn?titleId="+22896+"&seq=1";
			}else if ( (isLogin == 'true') && (isParty == 'true') ){
				location.href = "/"+levelName+"/detail.nhn?titleId="+22896+"&seq=1"; 
		    }else{
		    	var params = {
	  			  popupType   : 'C',
	  			  callbackUrl  : '',
	  			  serviceId   : 'webtoon',
	  			  feedGroupId  : 'comic',
	  			  feedGroupName  : 'comic',
	  			  feedId    : ['22896'],
	  			  feedName   : ["핑크레이디"],
	  			  feedUrl   : ["http://comic.naver.com/webtoon/list.nhn?titleId=22896"],
	  			  selected   : ['Y']
	  			};
	  			//popupObject.popupFeedWindow(params);
	  			popupObject.popupFeedWindow(params,{charEnc:'UTF-8',feedPopupHost:'relation.commcast.naver.com'});  
		    }
		}else{
			var params = {
			  popupType   : 'C',
			  callbackUrl  : '',
			  serviceId   : 'webtoon',
			  feedGroupId  : 'comic',
			  feedGroupName  : 'comic',
			  feedId    : ['22896'],
			  feedName   : ["핑크레이디"],
			  feedUrl   : ["http://comic.naver.com/webtoon/list.nhn?titleId=22896"],
			  selected   : ['Y']
			};
			//popupObject.popupFeedWindow(params);  
			popupObject.popupFeedWindow(params,{charEnc:'UTF-8',feedPopupHost:'relation.commcast.naver.com'});
		}		
	}

	//네이버미 레이어 노출
	function fn_navermeLayerShow(divId) {
		$Element(divId).show();
	}

	function fn_navermeLayerHide(divId) {
		$Element(divId).hide();
	}
		
	$Fn(loadThisWindow).attach(window,"load");

	
