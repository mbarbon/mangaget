<script type="text/javascript">
jindo.$Fn(function(){ 
	sps.config.template('countSimple','<h3 class="cb_h_type"><img src="http://static.comic.naver.com/staticImages/COMICWEB/NAVER/img/detail/h_comment.gif" width="51" height="14" alt="의견쓰기"><span>(<strong>{=count}</strong>)</span></h3>');
	sps.config.template('spanWriteBox','cbox_tpl_spanWriteBox');
	sps.config.template('policyDesc','cbox_tpl_policyDesc');
	sps.config.template('mobileIcon','cbox_tpl_mobileIcon');
	sps.config.template('mobileLayer','cbox_tpl_mobileLayer');
