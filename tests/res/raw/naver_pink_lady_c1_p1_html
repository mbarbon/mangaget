	// URL 복사
	function copyThisURL ()
	{
		
		if ( window.clipboardData)
		{
			 $Ajax("http://comic.naver.com/webtoon/copyUrl.nhn").request({"titleId":"22896","no":"1"});
			 window.clipboardData.setData('Text','http://comic.naver.com/webtoon/detail.nhn?titleId=22896&no=1' );		 
			 return true;
		}		
		else
		{
			alert('복사 기능을 지원하지 않는 브라우저입니다.');
			return false;
		}
	}		
	
	function isNavigationLeftEnd(current) {
		if (current - 3 > 0 ) 
			return true;
		else
			return false;
	}
	
	function isNavigationRightEnd(current, count) {	
		if ( current + 2 < count)
			return true;
		else
			return false;
	}
	
	function resizeReplyBox(width,height) {
		var replyIFrameBox = document.getElementById('replyIFrameBox');
		replyIFrameBox.width = width;
		replyIFrameBox.height = height;
	}
	
	function run_scrap(target) {
		
		
		if( isScrap == 'N' ) {
			alert("애니메이션은 스크랩하실 수 없습니다. ");
			return false;		
		}
		
		if ( !isLogin)
		{
			if ( !confirm('로그인 해야 이용할 수 있습니다. 로그인 하겠습니까?'))
				return false;
			else{
			
				var currentUrl = location.href;
				location.href="http://nid.naver.com/nidlogin.login?mode=form&url="+encodeURIComponent(currentUrl + "#putActionMenu");
				return ;
			}
		}
		var form = document.scrap_form;
		var element = document.getElementById('scrap_mehtod_name');
		if ( target == 'cafe')
			element.value ='cafe';
		else
			element.value = 'blog';
			
		nwin=open("about:blank", "comic_scrap", "top=10,left=10,toolbar=no,location=0,directories=0,status=yes,menubar=0,scrollbars=no,resizable=0,width=500,height=360");
		
		form.target = "comic_scrap";
		nwin.blur();
		nwin.focus();
		form.submit() ;
		
	}
	
	function request_fancafe() {
		
		
		if ( !isLogin)
		{
			if ( !confirm('로그인 해야 이용할 수 있습니다. 로그인 하겠습니까?'))
				return ;
				
			document.request_fancafe_form.refresh.value = "true";
		}
		var form = document.request_fancafe_form;
		nwin=open("about:blank", "request_fancafe", "top=10,left=10,toolbar=no,location=0,directories=0,status=yes,menubar=0,scrollbars=no,resizable=0,width=500,height=212");
		
		form.target = "request_fancafe";
		nwin.blur();


		nwin.focus();
		form.submit() ;	
		
	}
	
	function loadThisWindow () {
		new SearchBar('제목/작가로 검색할 수 있습니다.');			
		new RealtimeRankLayer();
		new RecommandRankLayer();	
	 	new FreeComicShowHideEventRegister(newButtonObject);
	 	new FreeComicEventDispatcher(22896,1,1 ,"WEBTOON",
	 	"연우 / 연우","u",
	 	"88",
	 	"story",
	 	"webtoon",
	 	isLogin );	
		
		new StarScorePoll(isLogin, oPageInfo);

		//$Element("tagBox").css("position","relative");
		//$Element("tagBox").css("zIndex","100");

		if (true)
		{
			$Element("comic_before").css("visibility","hidden");
		}
		if (false)
		{
			$Element("comic_after").css("visibility","hidden");
		}

		new NavigationBar(navi_init,"/navigationToJson.nhn?ln=WEBTOON&titleId=22896&weekday=mon");
		
	   	new ExtraRemover();
	   	
	   	

		if ( $("uploadLinkId") ){
			$Fn(checkUploadCertain).attach($("uploadLinkId"),"click");
		}

		if ( readCookie("visibleRemocon") != null &&  readCookie("visibleRemocon") == "false" )
			return ;

		//리모콘.
		$("comicRemocon").style.display = 'block';	
        var remoconOffset = $Element("comicRemocon").offset();
        var initLeft = remoconOffset.left;
        var initTop = remoconOffset.top;
	  	var height = 418;	

		eraseCookie("top");
		eraseCookie("left");
        var remocon ;
        if(isIE6HighOrOtherBrowser()){
	       	remocon = new RemoconComic("comicRemocon",height);
        }else{
        	remocon = new LowIE6RemoconComic("comicRemocon",height); 
        }
        
        var drag = new Drag("comicRemocon",remocon,initLeft,initTop);
	}

	function goUp(){
	    var max = $Element('comic_view_area').offset().top;
	    window.scrollTo(0,0);
	}

	function goDown(){
    	var max =  $Element('comic_view_area').first().height();
	    window.scrollTo(0,max-($Element("comicRemocon").height()));
    }

    function goPainterListByPainterId(painterId) {
        location.href = "/paintersTitle.nhn?painterId=" + encodeURIComponent(painterId);
    }

    function doComicReport(){
        try {
			var userId = "";
	        var painterId = "연우 / 연우";

	        if (!isLogin) {
                throw "NL"; // 로그인이 필요.
            } 
	
			if(userId == painterId) {
				throw "EP"; // 본인글은 신고할 수 없음.
			}

	    	var form = document.reportForm;
			var request_report = open("about:blank", "request_report", "top=10,left=10,toolbar=no,location=0,directories=0,status=yes,menubar=0,scrollbars=no,resizable=0,width=606,height=491");
			form.target = "request_report";
			form.submit();
        } catch (e) {
            if(e == "NL") {
            	if( confirm('로그인 해야 이용할 수 있습니다. 로그인 하시겠습니까?') ) {
                    location.href = "http://nid.naver.com/nidlogin.login?mode=form&url="+encodeURIComponent(location.href);
                }
            } else if(e == "EP") {
            	alert("본인의 게시물을 신고할 수 없습니다.");
            }
        } finally {
            return false;
        }
    }
    
