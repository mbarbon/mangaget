	}

	function onRollingClick(sType){
		oRolling = new jindo.Rolling($("comic_body"), {
			nFPS : 100,
			nDuration : 100,
			sDirection : "horizontal",
			fEffect : jindo.Effect.linear
		});
		sDirectionType = sType;
		
		var elLeft = $$.getSingle(".left_on", $("comic_body"));
		var elRight = $$.getSingle(".right_on", $("comic_body"));
		var aFirstId = (!!$Element(elLeft).first()) ? $Element(elLeft).first().attr("href").match(/no=\d+/g) : null;
		var aLastId = null;
		if(!!$Element(elRight).first()){
			aLastId = $Element(elRight).first().attr("href").match(/no=\d+/g);
		}else{
			var welNode = $Element(elRight);
			while(!welNode.first()){
				welNode = welNode.prev();
			}
			aLastId = welNode.first().attr("href").match(/no=\d+/g);
		}
		var sFirstId = sLastId = null;
		
		if(!!aFirstId){
			sFirstId = aFirstId[0].replace(/no=/g, "");
		}
		if(!!aLastId){
			sLastId = aLastId[0].replace(/no=/g, "");
		}
 
		sWebtoonId = (sType == "left") ? sFirstId : sLastId;
 
		var sTemplate = "<li><a href=\"{link}\"><img src=\"{thumbnail}\" alt=\"{chapter}\" width=\"70\" height=\"42\">{title}</a></li>";
		var sUrl = oPageInfo.pagelink + "?ln=" + oPageInfo.type + "&titleId=" + oPageInfo.titleId + "&weekday=" + oPageInfo.weekday + "";
		
		new $Ajax(sUrl, {
			type : "jsonp",
			onload : function(oResponse){
				var oRes = oResponse.json();
				if(!!oRes.list){
					var aTemp = [];
					var aList = oRes.list;
					navi_init.list = aList;
					var welList = $Element("comic_body").first();
					
					if(elLeft){
						$Element(elLeft).removeClass("left_on");
					}else{
						return false;
					}
					if(elRight){
						$Element(elRight).removeClass("right_on");
					}else{
						return false;
					}
					
					//sWebtoonId = (sType == "left") ?  navi_init.list[0].id : navi_init.list[navi_init.length-1].id;
					
					for(var i = 0; i < aList.length; i++){
						aTemp.push(sTemplate.replace("{link}", aList[i].link).replace("{thumbnail}", aList[i].thumbnail).replace("{chapter}", aList[i].chapter).replace("{title}", aList[i].title));
					}
					aTemp = aTemp.reverse();
					var sJoinStr = aTemp.join("");
					var nLength = oRolling.getItems().length;
					if(!oRes.leftend && sType == "left"){
						welList.prependHTML(sJoinStr);
						if($Element(elLeft).prev()) $Element(elLeft).prev().prev().prev().addClass("left_on");
						if(nLength < 7) welList.last().addClass("right_on");
						else $Element(elRight).prev().prev().prev().addClass("right_on");
					}
					if(!oRes.rightend && sType == "right"){
						welList.appendHTML(sJoinStr);
						if($Element(elLeft).next()) $Element(elLeft).next().next().next().addClass("left_on");
						if($Element(elRight).next()) $Element(elRight).next().next().next().addClass("right_on");
					}
					
					// 좌측, 우측 컨텐츠 유무에 대한 값에 따라 버튼 토글
					if(oRes.leftend){
						$Element("comic_before").hide();
					}else{
						$Element("comic_before").show("block");
					}
					if(oRes.rightend){
						$Element("comic_after").hide();
					}else{
						$Element("comic_after").show("block");
					}
					
				}
			}
		}).request({ "type" : sType, "id" : sWebtoonId });
		
		if(sDirectionType == "left"){
			oRolling.moveBy(-3);
		}else{
			oRolling.moveBy(3);
		}
	}

	function fn_naverLogin(){
		if (!isLogin) {
            if( confirm('로그인 해야 이용할 수 있습니다. 로그인 하시겠습니까?') ) {
                location.href = "http://nid.naver.com/nidlogin.login?mode=form&url="+encodeURIComponent(location.href);
            }
        }
	}

	$Fn(loadThisWindow).attach(window,"load");

	artistAction = new Artist();
	actionRunner = new WebtoonArtistActionRunner("22896");
