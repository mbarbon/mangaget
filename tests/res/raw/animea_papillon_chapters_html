function setread(chapter)
{
	$.get("/setread.php", { chapterid: chapter, mangaid: "5338", action: "add" },
	   function(){
			$('#ch_'+chapter).attr("class", "read");
			$('#ch_'+chapter).css("display", "inline");
	   });
	return false;
}

