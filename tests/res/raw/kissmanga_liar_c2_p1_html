            var currImage = 0;            
            $('#divImage img[src=""]:first').attr('src', lstImages[currImage]);
            currImage++;
            $('#divImage img[src=""]:first').attr('src', lstImages[currImage]);
            currImage++;

            $('#divImage img').load(function(){            
                if (currImage < lstImages.length)
                {
                    $('#divImage img[src=""]:first').attr('src', lstImages[currImage]);
                    currImage++;
                }      
                else
                    $('#pLoading').hide();
            });         

            $('#selectReadType').change(function () {
                document.cookie = "vns_readType=" + $(this).val() + "; expires=Thu, 2 Aug 2020 20:47:11 UTC;path=/;domain=.kissmanga.com";
                window.location.reload();
            });

            $('.selectChapter').change(function () {
                location.href = 'http://kissmanga.com/Manga/Liar-Game/' + $(this).val();
            });

            $('.selectGroup').change(function () {
                location.href = 'http://kissmanga.com/Manga/' + $(this).val();
            });

            function NextChap() {
                $('#divImage').empty();
                $('#divMsg').css('display', 'block');
            }     
